{
  "version": 2,
  "builds": [
    {
      "src": "api/comics.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/stories.js", 
      "use": "@vercel/node"
    },
    {
      "src": "api/artwork.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/comics",
      "dest": "/api/comics.js"
    },
    {
      "src": "/api/stories",
      "dest": "/api/stories.js"
    },
    {
      "src": "/api/artwork",
      "dest": "/api/artwork.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
